[user]
	name = 
	email = 
[branch]
	autosetupmerge = true

[color]
	ui = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
   	frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[branch "master"]
	remote = origin
[alias]
        stat = status
        hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --decorate --date=short
        glog = log --graph --decorate --date=short

